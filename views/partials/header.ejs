<header id="top-header">
  <span class="siteName">
    <h1><a href="/" title="Return to home page">CSE Motors</a></h1>
  </span>
  <div id="tools">
    <% if (locals.loggedin) { %>
      <a href="/account" >Welcome, <%- locals.accountData.account_firstname %></a> 
      <a title="Click to log in" href="/account/"></a>
      <form class="logout" method="post" action="/logout">
        <button type="submit">Logout</button>
      </form>
    <% } else { %>
      <a title="Click to log in" href="/account/login">My Account</a>
    <% } %>
  </div>
</header>
